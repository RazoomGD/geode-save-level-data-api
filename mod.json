{
	"geode": "4.9.0",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074"
	},
	"id": "razoom.save_level_data_api",
	"name": "Save Level Data API",
	"version": "v1.0.3",
	"developer": "RaZooM",
	"description": "API for saving level-specific data locally or in text objects",

	"dependencies": {
		"cvolton.level-id-api": {
			"version": "v1.0.0",
			"importance": "required"
		}
	},

	"api": {
		"include": [
			"include/*.hpp"
		]
	},

	"tags": ["editor", "developer", "api"],

	"links": {
		"source": "https://github.com/RazoomGD/geode-save-level-data-api",
		"community": "https://discord.gg/wcWvtKHP8n"
	},

	"settings": {
		"debug-mode": {
			"name": "Text object at {0,0} (for developers)",
			"description": "Changes text object position to (0,0)",
			"type": "bool",
			"default": false
		}
	}
}
